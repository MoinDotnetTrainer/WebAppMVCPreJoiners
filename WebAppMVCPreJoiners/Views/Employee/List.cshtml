@model IEnumerable<DataAccessLayer.Models.Employees>

@{
    ViewData["Title"] = "List";
}

<h4>List</h4>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Ename)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Password)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Ename)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                    <a asp-action="Edit" asp-route-Eid="@item.Eid">Edit</a> |
                    <a asp-action="Details" asp-route-Eid="@item.Eid">Details</a> |
                    <a asp-action="Delete" asp-route-Eid="@item.Eid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
